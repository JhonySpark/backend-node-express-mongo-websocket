{
  "name": "typescript-node-express-mongodb-backend",
  "version": "4.1.0",
  "main": "index.ts",
  "author": "HADMARINE",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && pm2 start -i 0",
    "dev": "nodemon",
    "build": "babel src --out-dir dist --extensions '.ts'",
    "tsc": "tsc",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "test": "jest --watch --detectOpenHandles --forceExit --testTimeout=10000",
    "test-auto": "npx jest --config ./jest.config.js",
    "enc:env": "openssl aes-256-cbc -e -in .env -out .env.pub -md md5 -k $PROGRAM_ENV_SECRET_KEY",
    "dec:env": "openssl aes-256-cbc -d -in .env.pub -out .env -md md5 -k $PROGRAM_ENV_SECRET_KEY",
    "dec:env-m": "openssl aes-256-cbc -d -in .env.pub -out .env -md md5 -k",
    "berry": "yarn set version berry",
    "latest": "yarn set version latest",
    "classic": "yarn set version classic"
  },
  "dependencies": {
    "aws-sdk": "^2.715.0",
    "chalk": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "deasync": "^0.1.20",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.7-alpha.3",
    "express-ipfilter": "^1.1.2",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.21.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.7.11",
    "morgan": "^1.10.0",
    "node-cron": "^2.0.3",
    "nodemon": "^1.19.4",
    "pm2": "^4.4.0",
    "require-strip-json-comments": "^2.0.0",
    "socket.io": "^2.3.0",
    "strip-json-comments": "^3.1.0",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/deasync": "^0.1.1",
    "@types/express": "^4.17.8",
    "@types/express-fileupload": "^1.1.3",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.2.1",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.5.31",
    "@types/morgan": "^1.9.0",
    "@types/node": "^12.12.5",
    "@types/node-cron": "^2.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-jest": "^23.8.2",
    "jest": "^25.3.0",
    "prettier": "^2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.4.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0-beta0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
